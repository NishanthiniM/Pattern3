#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    int i=1;
    while(i<=n){
        int j=1;
        while(j<=n-i){
            printf(" ");
            j=j+1;
        }
        int k=1;
        while(k<=i){
            printf("%d",k);
            k=k+1;
            
        }
        printf("\n");
        i=i+1;
    }
 }
